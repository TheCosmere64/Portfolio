<?xml version="1.0" encoding="utf-8"?>

<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  x:Class="AIMS.Views.MasterDetailPages.HomePage"
                  xmlns:pages="clr-namespace:AIMS.Views.MasterDetailPages">
    <MasterDetailPage.Master>
        <pages:HomePageMaster x:Name="MasterPage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.ToolbarItems>
        <ToolbarItem Icon="Settings.png" Activated="OpenSettingsPopup" />
    </MasterDetailPage.ToolbarItems>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:HomePageDetail />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>